/*Bootstrapping the Mean.jslJune 20, 2008PurposeThis script constructs and displays the bootstrap distribution for the Mean.AuthorThomas F. Devlin Montclair State University*/Clear Globals();dlg = Column Dialog(		yCol = Col List( "Y, Column",			Data Type( Numeric ),			Min Col ( 1 ),			Max Col ( 1 )		),		Line Up( 2,			"Number of Resamples", ntrials = Edit Number(50)		));if( dlg["Button"] == -1, Throw ( "User cancelled" ) );Remove From(dlg);  Eval List ( dlg );dt = currentDataTable(); resultsTable = newTable("resampleResults.jmp", 		newColumn("Mean",numeric), 		newColumn("StdDev",numeric)		); freqCol = dt<<NewColumn("Resample",numeric,formula(ResampleFreq())); for(i=1,i<=ntrials,i++,   freqCol<<evalFormula; dt<<runFormulas;   obj = dt<<Distribution(invisible, Y( yCol[1] ), Freq(:Resample));   estimates = (obj<<report)["Moments", NumberColBox(1)] << get as matrix;   obj<<CloseWindow;   resultsTable<<AddRow( {Mean=estimates[1], StdDev=estimates[2]} );   wait(0); ); dt<<DeleteColumns({"Resample"});resultsTable<<Distribution(Y(:Mean, :StdDev));